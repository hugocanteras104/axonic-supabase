-- ===================================================
-- VERIFICACI√ìN FINAL
-- ===================================================

DO $$
DECLARE
  v_count_always int;
  v_count_features int;
  v_count_owner int;
  v_total int;
BEGIN
  SELECT COUNT(*) INTO v_count_always FROM global_tool_definitions WHERE tool_category = 'ALWAYS';
  SELECT COUNT(*) INTO v_count_features FROM global_tool_definitions WHERE tool_category LIKE 'FEATURES.%';
  SELECT COUNT(*) INTO v_count_owner FROM global_tool_definitions WHERE tool_category = 'OWNER';
  
  v_total := v_count_always + v_count_features + v_count_owner;
  
  RAISE NOTICE '';
  RAISE NOTICE '‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê';
  RAISE NOTICE 'üéâ SEED COMPLETADO - TODAS LAS HERRAMIENTAS CARGADAS';
  RAISE NOTICE '‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê';
  RAISE NOTICE '';
  RAISE NOTICE 'üìä RESUMEN FINAL:';
  RAISE NOTICE '   ALWAYS (Core): % herramientas', v_count_always;
  RAISE NOTICE '   FEATURES: % herramientas', v_count_features;
  RAISE NOTICE '   OWNER: % herramientas', v_count_owner;
  RAISE NOTICE '   ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ';
  RAISE NOTICE '   TOTAL: % herramientas ‚úÖ', v_total;
  RAISE NOTICE '';
  RAISE NOTICE 'üéØ SISTEMA LISTO PARA PRODUCCI√ìN';
  RAISE NOTICE '';
  RAISE NOTICE 'üß™ PRUEBA EL SISTEMA:';
  RAISE NOTICE '   SELECT * FROM search_available_tools(';
  RAISE NOTICE '     ''tu-business-id''::uuid,';
  RAISE NOTICE '     ''quiero agendar una cita'',';
  RAISE NOTICE '     ''lead'',';
  RAISE NOTICE '     10';
